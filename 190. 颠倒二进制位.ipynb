    "class Solution:\n",
    "    def reverseBits(self, n: int) -> int:\n",
    "        res = bin(n)[2:] #将n转换成2进制形式，并且去除前缀0b\n",
    "        res = res.zfill(32)#在二进制左侧填充0，使得长度为32位\n",
    "        res = res[::-1]# 反转二进制\n",
    "        return int(res,base=2)#将二进制转换为整数\n"
   ]
  }
