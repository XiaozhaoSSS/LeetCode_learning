   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def rob(self, nums: List[int]) -> int:\n",
    "        pre_max,curr_max=0,0\n",
    "        for num in nums:\n",
    "            temp=curr_max\n",
    "            curr_max=max(curr_max,pre_max+num)\n",
    "            pre_max=temp\n",
    "        return curr_max"
   ]
  }
