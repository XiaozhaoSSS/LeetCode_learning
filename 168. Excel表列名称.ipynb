   "source": [
    "class Solution:\n",
    "    def convertToTitle(self, n: int) -> str:\n",
    "        res = \"\"\n",
    "        while n:\n",
    "            n -= 1\n",
    "            n, y = divmod(n, 26) \n",
    "            res = chr(y + 65) + res\n",
    "        return res\n"
   ]
  }
