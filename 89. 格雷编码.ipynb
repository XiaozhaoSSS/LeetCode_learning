    "class Solution:\n",
    "    def grayCode(self, n: int) -> List[int]:\n",
    "        \n",
    "        res=[0,1,3,2]\n",
    "        if n<=2:return res[:2**n]\n",
    "        for i in range(2,n):\n",
    "            res+=[x+2**i for x in res[::-1]]\n",
    "        return res\n",
    "             "
   ]
